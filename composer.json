{
  "name": "bccampus/excel-export",
  "description": "Allows admins to export data to an excel file",
  "license": "GPL",
  "type": "wordpress-plugin",
  "version": "0.5.0",
  "homepage": "https://github.com/BCcampus/excel-export/",
  "authors": [
    {
      "name": "Alex Paredes",
      "homepage": "https://github.com/alex-418"
    },
    {
      "name": "Brad Payne",
      "homepage": "https://github.com/bdolor"
    }
  ],
  "keywords": [
    "excel",
    "export"
  ],
  "support": {
    "issues": "https://github.com/Bccampus/excel-export/issues",
    "source": "https://github.com/BCcampus/excel-export/"
  },
  "config": {
    "platform": {
      "php": "7.1"
    }
  },
  "require": {
    "php": ">=7.0",
    "phpoffice/phpspreadsheet": "^1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5",
    "humanmade/coding-standards": "^0.5"
  },
  "archive": {
    "exclude": [
      ".gitignore",
      ".travis.yml",
      "phpunit.xml",
      "tests"
    ]
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit --configuration phpunit.xml ."
    ],
    "standards": [
      "vendor/bin/phpcs --standard=phpcs.ruleset.xml ."
    ]
  }
}
